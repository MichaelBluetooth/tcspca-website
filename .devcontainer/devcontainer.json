{
	"name": "Node.js & TypeScript",
	"image": "mcr.microsoft.com/devcontainers/typescript-node:1-20-bookworm",
	"features": {
		"ghcr.io/devcontainers-extra/features/angular-cli:2": {},
		"ghcr.io/devcontainers/features/github-cli": {}
	},
	// "features": {},
	"forwardPorts": [
		4200
	],
	"postCreateCommand": "bash ./tcspca-website/.devcontainer/postCreateCommand.sh",
	"postStartCommand": "bash ./tcspca-website/.devcontainer/postStartCommand.sh",
	"customizations": {
